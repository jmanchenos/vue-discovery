{
 "name": "vue-discovery",
 "displayName": "Vue discovery",
 "description": "Missing component discovery for Vue.",
 "publisher": "maantje",
 "version": "0.1.0",
 "engines": {
  "vscode": "^1.44.0"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [
  "*"
 ],
 "main": "./src/extension.js",
 "contributes": {
  "configuration": {
   "type": "object",
   "title": "Vue.js discovery",
   "properties": {
    "vueDiscovery.rootDirectory": {
     "type": "string",
     "default": "",
     "description": "root directory in workspace for file grep. ex) src"
    }
   }
  },
  "keybindings": [
    {
      "command": "vueDiscovery.importExisting",
      "key": "ctrl+i",
      "mac": "cmd+i",
      "when": "editorTextFocus"
    }
  ],
  "commands": [
    {
      "command": "vueDiscovery.importExisting",
      "title": "Vue Discovery: Import a vue component"
    }
  ]
 },
 "scripts": {
  "lint": "eslint .",
  "pretest": "yarn run lint",
  "test": "node ./test/runTest.js"
 },
 "devDependencies": {
  "@types/glob": "^7.1.1",
  "@types/mocha": "^7.0.2",
  "@types/node": "^13.11.0",
  "@types/vscode": "^1.44.0",
  "eslint": "^6.8.0",
  "eslint-config-spatie": "^2.0.3",
  "mocha": "^7.1.1",
  "typescript": "^3.8.3",
  "vscode-test": "^1.3.0"
 },
 "dependencies": {
  "glob": "^7.1.6",
  "vue-parser": "^1.1.6"
 }
}
